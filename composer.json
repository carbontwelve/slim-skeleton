{
  "name": "carbontwelve/slim-skeleton",
  "description": "An opinionated slim skeleton",
  "keywords": ["framework", "slim", "mvc", "slim-starter"],
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Dann",
      "email": "simon.dann@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "slim/slim": "^3.1",
    "monolog/monolog": "^1.17",
    "aura/session": "^2.0",
    "carbontwelve/slim-plates": "^0.0.1"
  },
  "require-dev": {
    "zeuxisoo/slim-whoops": "0.5.*",
    "symfony/var-dumper": "^3.0"
    },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
